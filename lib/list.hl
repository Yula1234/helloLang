cextern "init_list_hl"
cextern "add_list_hl"
cextern "get_list_hl"

def init_list(void) -> ptr {
	ptr p = __cast_p(0)
	__asm__ "call init_list_hl
	mov dword [ebp-8], eax"
	return p
}

def add_list(lst:ptr, value:int) -> void {
	__asm__ "push dword [ebp-8]
	push dword [ebp-12]
	call add_list_hl
	add esp, 8"
}

def get_list(lst:ptr, index:int) -> int {
	int res = 0
	__asm__ "push dword [ebp-8]
	push dword [ebp-12]
	call get_list_hl
	add esp, 8
	mov dword [ebp-16], eax"
	return res
}

cextern "file_write_hl"
cextern "file_read_hl"

def f_write(path:ptr,value:ptr) -> void {
	__asm__ "push dword [ebp-12]
	push dword [ebp-8]
	call file_write_hl
	add esp, 8"
}

def f_read(path:ptr) -> ptr {
	ptr res = __cast_p(0)
	__asm__ "push dword [ebp-8]
	call file_read_hl
	add esp, 4
	mov dword [ebp-12], eax"
	return res
}
